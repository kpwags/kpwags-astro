{
    "extends": "astro/tsconfigs/strict",
    "compilerOptions": {
        "strictNullChecks": true,
        "baseUrl": ".",
        "paths": {
            "@components/*": ["src/components/*"],
            "@data/*": ["src/data/*"],
            "@layouts/*": ["src/layouts/*"],
            "@lib/*": ["src/lib/*"],
            "@models/*": ["src/models/*"],
            "@styles/*": ["src/styles/*"]
        }
    }
}
